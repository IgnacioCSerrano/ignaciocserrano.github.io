let tablaMultiplicarUL = n => {
    let lista = "";
    if (Number.isInteger(Number(n))) {
        lista = "<ul>\n";
        for (let i = 0; i <= 10; i++) {
            lista += `    <li>${n} x ${i} = ${n * i}</li>\n`;
        }
        lista += "</ul>";
    } else {
        console.error("Error: parámetro no es un número entero");
    }
    return lista;
};
